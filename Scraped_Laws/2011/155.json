  "preamble": "أقر مجلس النواب،\nوينشر رئيس الجمهورية القانون التالي نصه:",
  "articles": [
    {
      "number": "2 يعمل بهذا القانون فور نشره في الجريدة الرسمية.",
      "content": [
        {
          "type": "text",
          "data": "يعدل تصنيف الأوسمة الوطنية موضوع المادة الأولى من المرسوم الاشتراعي رقم 122 تاريخ 12/6/1959 وتعديلاته بحيث يصبح وفقا للترتيب التالي:"
        },
        {
          "type": "text",
          "data": "1- وسام الاستحقاق اللبناني"
        },
        {
          "type": "text",
          "data": "2- وسام الارز الوطني"
        },
        {
          "type": "text",
          "data": "3- وسام المغترب"
        },
        {
          "type": "text",
          "data": "4- وسام الفخر العسكري"
        },
        {
          "type": "text",
          "data": "5- الميدالية العسكرية"
        },
        {
          "type": "text",
          "data": "6- الوسام الحربي"
        },
        {
          "type": "text",
          "data": "7- وسام النسر للطيران الوسام البحري"
        },
        {
          "type": "text",
          "data": "8- وسام التقدير العسكري"
        },
        {
          "type": "text",
          "data": "9- ميدالية الجدارة"
        },
        {
          "type": "text",
          "data": "10- وسام الجرحى"
        },
        {
          "type": "text",
          "data": "11- ميدالية وزارة الداخلية والبلديات"
        },
        {
          "type": "text",
          "data": "12- الأوسمة التذكارية للعمليات العسكرية - ميدالية الجهاد الوطني - ميدالية فلسطين التذكارية - وسام 31 كانون الأول 1961 التذكاري - وسام الوحدة الوطنية - وسام فجر الجنوب - الميدالية الوطنية للمؤتمرات"
        },
        {
          "type": "text",
          "data": "13- وسام العمل"
        },
        {
          "type": "text",
          "data": "14- وسام المعارف"
        },
        {
          "type": "text",
          "data": "15- وسام الاستحقاق الصحي"
        },
        {
          "type": "text",
          "data": "16- وسام الاستحقاق الزراعي"
        },
        {
          "type": "text",
          "data": "17- وسام الأسير"
        },
        {
          "type": "text",
          "data": "18- ميدالية الأمن العام"
        },
        {
          "type": "text",
          "data": "19- وسام المعلم"
        },
        {
          "type": "text",
          "data": "20- ميدالية أمن الدولة"
        },
        {
          "type": "text",
          "data": "21- ميدالية الأمن الداخلي"
        }
      ]
    },
    {
      "number": "1",
      "content": [
        {
          "type": "text",
          "data": "يعمل بهذا القانون فور نشره في الجريدة الرسمية."
        }
      ]
    }
  ],
