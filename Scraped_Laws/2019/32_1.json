  "preamble": "إن رئيس هيئة الأسواق المالية،\nبناء على القانون رقم 234 تاريخ 10/6/2000 لا سيما المادة السادسة منه،\nوبناء على القانون رقم 161 تاريخ 17/8/2011،\nيُقرّر ما يأتي:",
  "articles": [
    {
      "number": "2 يُنشر هذا القرار في الجريدة الرسمية ويُبلّغ حيث تدعو الحاجة.",
      "content": [
        {
          "type": "text",
          "data": "تنشر كما يلي لائحة مؤسسات الوساطة المالية المسجلة حسب الأصول:"
        },
        {
          "type": "text",
          "data": "1"
        },
        {
          "type": "text",
          "data": "- جوليوس بير (ليبانون) ش.م.ل."
        },
        {
          "type": "text",
          "data": "2"
        },
        {
          "type": "text",
          "data": "3"
        },
        {
          "type": "text",
          "data": "4"
        },
        {
          "type": "text",
          "data": "5"
        },
        {
          "type": "text",
          "data": "- الشركة العربية الدولية للانماء والاستثمار ش.م.ل."
        },
        {
          "type": "text",
          "data": "6"
        },
        {
          "type": "text",
          "data": "7"
        },
        {
          "type": "text",
          "data": "8"
        },
        {
          "type": "text",
          "data": "- شركة بيروت للوساطة المالية ش.م.ل."
        },
        {
          "type": "text",
          "data": "9"
        },
        {
          "type": "text",
          "data": "10"
        },
        {
          "type": "text",
          "data": "11"
        },
        {
          "type": "text",
          "data": "12"
        },
        {
          "type": "text",
          "data": "13"
        },
        {
          "type": "text",
          "data": "- شركة ليبانيز ديلرز ش.م.ل."
        },
        {
          "type": "text",
          "data": "14"
        },
        {
          "type": "text",
          "data": "15"
        },
        {
          "type": "text",
          "data": "- رويال فاينانشيلز ش.م.ل."
        },
        {
          "type": "text",
          "data": "16"
        },
        {
          "type": "text",
          "data": "- ماستر كابيتال غروب ش.م.ل."
        },
        {
          "type": "text",
          "data": "17"
        },
        {
          "type": "text",
          "data": "- شركة الدقة للاستثمار والوساطة المالية ش.م.ل."
        },
        {
          "type": "text",
          "data": "18"
        },
        {
          "type": "text",
          "data": "19"
        },
        {
          "type": "text",
          "data": "20"
        },
        {
          "type": "text",
          "data": "- تراست كابيتال ش.م.ل."
        },
        {
          "type": "text",
          "data": "21"
        },
        {
          "type": "text",
          "data": "-"
        },
        {
          "type": "text",
          "data": "22"
        },
        {
          "type": "text",
          "data": "- بلوم لإدارة الأصول ش.م.ل."
        },
        {
          "type": "text",
          "data": "23"
        },
        {
          "type": "text",
          "data": "- برودغايت أدفايسرز ش.م.ل."
        },
        {
          "type": "text",
          "data": "24"
        },
        {
          "type": "text",
          "data": "- أر.أم. لإدارة الثروات الخاصة ش.م.ل."
        },
        {
          "type": "text",
          "data": "25"
        },
        {
          "type": "text",
          "data": "- كابيتال إي ش.م.ل."
        },
        {
          "type": "text",
          "data": "26"
        },
        {
          "type": "text",
          "data": "- أمانة كابيتال ش.م.ل."
        },
        {
          "type": "text",
          "data": "27"
        },
        {
          "type": "text",
          "data": "- أيش. أس. بي. سي. فينانشيل سيرفيزيز (لبنان) ش.م.ل."
        },
        {
          "type": "text",
          "data": "28"
        },
        {
          "type": "text",
          "data": "- سرادار فاميلي أوفيس (أس. أف. أو) ش.م.ل."
        },
        {
          "type": "text",
          "data": "29"
        },
        {
          "type": "text",
          "data": "- فالبري كابيتال ش.م.ل."
        },
        {
          "type": "text",
          "data": "30 - شناندووا كنسلتينغ ش.م.ل."
        }
      ]
    },
    {
      "number": "1",
      "content": [
        {
          "type": "text",
          "data": "يُنشر هذا القرار في الجريدة الرسمية ويُبلّغ حيث تدعو الحاجة."
        }
      ]
    }
  ],
