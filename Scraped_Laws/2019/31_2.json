  "preamble": "ان رئيس هيئة الاسواق المالية،\nبناء على القانون رقم 234 تاريخ 10/6/2000 لا سيما المادة السادسة منه،\nوبناء على القانون رقم 161 تاريخ 17/8/2011،\nيقرر ما يأتي:",
  "articles": [
    {
      "number": "2 ينشر هذا القرار في الجريدة الرسمية ويبلغ حيث تدعو الحاجة.",
      "content": [
        {
          "type": "text",
          "data": "تنشر كما يلي لائحة مؤسسات الوساطة المالية المسجلة حسب الاصول:"
        },
        {
          "type": "text",
          "data": "1- جوليوس بير (ليبانون) ش.م.ل."
        },
        {
          "type": "text",
          "data": "2-"
        },
        {
          "type": "text",
          "data": "3-"
        },
        {
          "type": "text",
          "data": "4-"
        },
        {
          "type": "text",
          "data": "5- الشركة العربية الدولية للانماء والاستثمار ش.م.ل."
        },
        {
          "type": "text",
          "data": "6-"
        },
        {
          "type": "text",
          "data": "7-"
        },
        {
          "type": "text",
          "data": "8- شركة بيروت للوساطة المالية ش.م.ل."
        },
        {
          "type": "text",
          "data": "9-"
        },
        {
          "type": "text",
          "data": "10-"
        },
        {
          "type": "text",
          "data": "11-"
        },
        {
          "type": "text",
          "data": "12-"
        },
        {
          "type": "text",
          "data": "13- شركة ليبانيز ش.م.ل."
        },
        {
          "type": "text",
          "data": "14-"
        },
        {
          "type": "text",
          "data": "15- رويال فاينانشيلز ش.م.ل."
        },
        {
          "type": "text",
          "data": "16- ماستر كابيتال غروف ش.م.ل."
        },
        {
          "type": "text",
          "data": "17- شركة الدقة للاستثمار والوساطة المالية ش.م.ل."
        },
        {
          "type": "text",
          "data": "18-"
        },
        {
          "type": "text",
          "data": "19-"
        },
        {
          "type": "text",
          "data": "20- تراست كابيتال ش.م.ل."
        },
        {
          "type": "text",
          "data": "21-"
        },
        {
          "type": "text",
          "data": "22- بلوم لادارة الاصول ش.م.ل."
        },
        {
          "type": "text",
          "data": "23- برودغايت ادفايسرز ش.م.ل."
        },
        {
          "type": "text",
          "data": "24- أر. أم. لادارة الثروات الخاصة ش.م.ل."
        },
        {
          "type": "text",
          "data": "25- كابيتال إي ش.م.ل."
        },
        {
          "type": "text",
          "data": "26- امانة كابيتال ش.م.ل."
        },
        {
          "type": "text",
          "data": "27- أيش.أس.بي.سي. فينانشيل سيرفيزيز (لبنان) ش.م.ل."
        },
        {
          "type": "text",
          "data": "28- سرادار فاميلي اوفيس (أس.أف.أو) ش.م.ل."
        },
        {
          "type": "text",
          "data": "29- فالبري كابيتال ش.م.ل."
        }
      ]
    },
    {
      "number": "1",
      "content": [
        {
          "type": "text",
          "data": "ينشر هذا القرار في الجريدة الرسمية ويبلغ حيث تدعو الحاجة."
        }
      ]
    }
  ],
  "signature": "بيروت، في 13 حزيران 2019\nرئيس هيئة الاسواق المالية حاكم مصرف لبنان\nرياض توفيق سلامه"
}