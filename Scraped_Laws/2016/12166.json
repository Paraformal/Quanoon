  "preamble": "إن حاكم مصرف لبنان،\nبناء على قانون النقد والتسليف لا سيما المادة 136 فقرتها الثانية منه،\nيُقرّر ما يأتي:",
  "articles": [
    {
      "number": "الثانية: يُنشر هذا القرار في الجريدة الرسمية ويُبلّغ حيث تدعو الحاجة.",
      "content": [
        {
          "type": "text",
          "data": "المادة الأولى: تنشر كما يلي لائحة المصارف المسجلة حسب الأصول:"
        },
        {
          "type": "text",
          "data": "1\n- فرنسبنك ش.م.ل."
        },
        {
          "type": "text",
          "data": "2"
        },
        {
          "type": "text",
          "data": "3\n- بنك مصر لبنان ش.م.ل."
        },
        {
          "type": "text",
          "data": "4\n-"
        },
        {
          "type": "text",
          "data": "5\n- البنك العربي ش.م.ع."
        },
        {
          "type": "text",
          "data": "6"
        },
        {
          "type": "text",
          "data": "7\n- بنك اتش اس بي سي الشرق الأوسط المحدود."
        },
        {
          "type": "text",
          "data": "8"
        },
        {
          "type": "text",
          "data": "9\n- مصرف الرافدين"
        },
        {
          "type": "text",
          "data": "10\n- البنك اللبناني الفرنسي ش.م.ل."
        },
        {
          "type": "text",
          "data": "11\n- البنك اللبناني للتجارة ش.م.ل."
        },
        {
          "type": "text",
          "data": "12\n- البنك التجاري للشرق الأدنى ش.م.ل."
        },
        {
          "type": "text",
          "data": "13"
        },
        {
          "type": "text",
          "data": "14\n- بنك لبنان والمهجر ش.م.ل."
        },
        {
          "type": "text",
          "data": "15"
        },
        {
          "type": "text",
          "data": "16\n- بنك فدرال لبنان ش.م.ل."
        },
        {
          "type": "text",
          "data": "17\n- البنك الأهلي التجاري السعودي"
        },
        {
          "type": "text",
          "data": "18"
        },
        {
          "type": "text",
          "data": "19\n- بنك سوسيته جنرال في لبنان ش.م.ل."
        },
        {
          "type": "text",
          "data": "20"
        },
        {
          "type": "text",
          "data": "21"
        },
        {
          "type": "text",
          "data": "22\n- بنك البحر المتوسط ش.م.ل."
        },
        {
          "type": "text",
          "data": "23"
        },
        {
          "type": "text",
          "data": "24"
        },
        {
          "type": "text",
          "data": "25"
        },
        {
          "type": "text",
          "data": "26"
        },
        {
          "type": "text",
          "data": "27\n- بنك عوده للخدمات الخاصة ش.م.ل"
        },
        {
          "type": "text",
          "data": "28\n- بنك بيروت والبلاد العربية ش.م.ل."
        },
        {
          "type": "text",
          "data": "29"
        },
        {
          "type": "text",
          "data": "30"
        },
        {
          "type": "text",
          "data": "31"
        },
        {
          "type": "text",
          "data": "32"
        },
        {
          "type": "text",
          "data": "33\n- بنك عوده للأعمال ش.م.ل."
        },
        {
          "type": "text",
          "data": "34\n- المصرف التجاري السوري اللبناني ش.م.ل."
        },
        {
          "type": "text",
          "data": "35\n- بنك فرعون وشيحا ش.م.ل."
        },
        {
          "type": "text",
          "data": "36\n- بنك الإعتماد الوطني ش.م.ل."
        },
        {
          "type": "text",
          "data": "37"
        },
        {
          "type": "text",
          "data": "38"
        },
        {
          "type": "text",
          "data": "39\n- بنك بيبلوس ش.م.ل."
        },
        {
          "type": "text",
          "data": "40"
        },
        {
          "type": "text",
          "data": "41\n- مصرف الإسكان ش.م.ل."
        },
        {
          "type": "text",
          "data": "42"
        },
        {
          "type": "text",
          "data": "43\n- بنك التمويل ش.م.ل."
        },
        {
          "type": "text",
          "data": "44"
        },
        {
          "type": "text",
          "data": "45"
        },
        {
          "type": "text",
          "data": "46"
        },
        {
          "type": "text",
          "data": "47"
        },
        {
          "type": "text",
          "data": "48\n- بنك الصناعة والعمل ش.م.ل."
        },
        {
          "type": "text",
          "data": "49"
        },
        {
          "type": "text",
          "data": "50"
        },
        {
          "type": "text",
          "data": "51"
        },
        {
          "type": "text",
          "data": "52\n- بنك انتركونتيننتال لبنان ش.م.ل."
        },
        {
          "type": "text",
          "data": "53\n- الإعتماد اللبناني ش.م.ل."
        },
        {
          "type": "text",
          "data": "54"
        },
        {
          "type": "text",
          "data": "55"
        },
        {
          "type": "text",
          "data": "56\n- بنك عوده ش.م.ل"
        },
        {
          "type": "text",
          "data": "57"
        },
        {
          "type": "text",
          "data": "58\n- فينيسيا بنك ش.م.ل"
        },
        {
          "type": "text",
          "data": "59"
        },
        {
          "type": "text",
          "data": "60"
        },
        {
          "type": "text",
          "data": "61"
        },
        {
          "type": "text",
          "data": "62\n- مصرف شمال افريقيا التجاري ش.م.ل."
        },
        {
          "type": "text",
          "data": "63\n- البنك اللبناني السويسري ش.م.ل."
        },
        {
          "type": "text",
          "data": "64\n-"
        },
        {
          "type": "text",
          "data": "65"
        },
        {
          "type": "text",
          "data": "66"
        },
        {
          "type": "text",
          "data": "67\n- بنك صادرات إيران"
        },
        {
          "type": "text",
          "data": "68\n- الشركة الجديدة لبنك سوريا ولبنان ش.م.ل."
        },
        {
          "type": "text",
          "data": "69"
        },
        {
          "type": "text",
          "data": "70"
        },
        {
          "type": "text",
          "data": "71"
        },
        {
          "type": "text",
          "data": "72"
        },
        {
          "type": "text",
          "data": "73\n- بنك الكويت الوطني (لبنان) ش.م.ل."
        },
        {
          "type": "text",
          "data": "74\n-"
        },
        {
          "type": "text",
          "data": "75\n- بنك بيروت ش.م.ل."
        },
        {
          "type": "text",
          "data": "76"
        },
        {
          "type": "text",
          "data": "77"
        },
        {
          "type": "text",
          "data": "78"
        },
        {
          "type": "text",
          "data": "79"
        },
        {
          "type": "text",
          "data": "80\n- جمال ترست بنك ش.م.ل."
        },
        {
          "type": "text",
          "data": "81"
        },
        {
          "type": "text",
          "data": "82"
        },
        {
          "type": "text",
          "data": "83"
        },
        {
          "type": "text",
          "data": "84"
        },
        {
          "type": "text",
          "data": "85\n- حبيب بنك ليمتد"
        },
        {
          "type": "text",
          "data": "86"
        },
        {
          "type": "text",
          "data": "87"
        },
        {
          "type": "text",
          "data": "88"
        },
        {
          "type": "text",
          "data": "89"
        },
        {
          "type": "text",
          "data": "90\n- البنك العربي الافريقي الدولي"
        },
        {
          "type": "text",
          "data": "91"
        },
        {
          "type": "text",
          "data": "92\n- بنك الامارات ولبنان ش.م.ل"
        },
        {
          "type": "text",
          "data": "93\n- بنك بيمو ش.م.ل"
        },
        {
          "type": "text",
          "data": "94\n- بنك لبنان والخليج ش.م.ل."
        },
        {
          "type": "text",
          "data": "95\n- البنك السعودي اللبناني ش.م.ل."
        },
        {
          "type": "text",
          "data": "96"
        },
        {
          "type": "text",
          "data": "97"
        },
        {
          "type": "text",
          "data": "98\n- سيدروس بنك ش.م.ل"
        },
        {
          "type": "text",
          "data": "99"
        },
        {
          "type": "text",
          "data": "100"
        },
        {
          "type": "text",
          "data": "101\n- بنك الموارد ش.م.ل."
        },
        {
          "type": "text",
          "data": "102"
        },
        {
          "type": "text",
          "data": "103\n- الإعتماد المصرفي ش.م.ل."
        },
        {
          "type": "text",
          "data": "104\n- بنك الاعتماد المتحد ش.م.ل."
        },
        {
          "type": "text",
          "data": "105\n- بنك المدينة ش.م.ل."
        },
        {
          "type": "text",
          "data": "106"
        },
        {
          "type": "text",
          "data": "107"
        },
        {
          "type": "text",
          "data": "108\n- فرست ناشونال بنك ش.م.ل."
        },
        {
          "type": "text",
          "data": "109\n- بنك البركة ش.م.ل."
        },
        {
          "type": "text",
          "data": "110\n- بنك الشرق الأوسط وافريقيا ش.م.ل."
        },
        {
          "type": "text",
          "data": "111\n- بنك لبنان والمهجر للأعمال ش.م.ل."
        },
        {
          "type": "text",
          "data": "112"
        },
        {
          "type": "text",
          "data": "113\n- بنك البحر المتوسط للاستثمار ش.م.ل."
        },
        {
          "type": "text",
          "data": "114\n- بنك الاعتماد اللبناني للاستثمار ش.م.ل."
        },
        {
          "type": "text",
          "data": "115\n- سيتي بنك ن.أ."
        },
        {
          "type": "text",
          "data": "116"
        },
        {
          "type": "text",
          "data": "117"
        },
        {
          "type": "text",
          "data": "118\n- البنك العربي للاعمال ش.م.ل"
        },
        {
          "type": "text",
          "data": "119"
        },
        {
          "type": "text",
          "data": "120"
        },
        {
          "type": "text",
          "data": "121\n- فرنسبنك للاعمال ش.م.ل."
        },
        {
          "type": "text",
          "data": "122\n-"
        },
        {
          "type": "text",
          "data": "123\n- بنك بيبلوس للاعمال ش.م.ل."
        },
        {
          "type": "text",
          "data": "124"
        },
        {
          "type": "text",
          "data": "125\n- بيت التمويل العربي ش.م.ل (مصرف إسلامي)"
        },
        {
          "type": "text",
          "data": "126\n- البنك الإسلامي اللبناني ش.م.ل"
        },
        {
          "type": "text",
          "data": "127\n- بنك بلوم للتنمية ش.م.ل"
        },
        {
          "type": "text",
          "data": "128"
        },
        {
          "type": "text",
          "data": "129\n- «إف. إف. إي» ش.م.ل. (مصرف متخصص"
        },
        {
          "type": "text",
          "data": "130\n- بنك بيروت للاستثمار ش.م.ل"
        },
        {
          "type": "text",
          "data": "131"
        },
        {
          "type": "text",
          "data": "132\n- شركة مصرف بغداد (مساهمة خاصة)"
        },
        {
          "type": "text",
          "data": "133\n- بنك سي.أس.سي ش.م.ل."
        },
        {
          "type": "text",
          "data": "134\n- شركة مصرف البلاد الاسلامي للاستثمار والتمويل (مساهمة خاصة)"
        },
        {
          "type": "text",
          "data": "135\n- بنك انتركونتيننتال لبنان للاستثمار ش.م.ل"
        },
        {
          "type": "text",
          "data": "136\n- بنك قطر الوطني (شركة مساهمة قطرية)"
        },
        {
          "type": "text",
          "data": "137\n- سيدروس إنفست بنك ش.م.ل"
        },
        {
          "type": "text",
          "data": "138\n- ب.ل.س. اينفست ش.م.ل"
        },
        {
          "type": "text",
          "data": "139\n- لي بنك ش.م.ل (ليفنت انفستمنت بنك)"
        },
        {
          "type": "text",
          "data": "140\n-"
        },
        {
          "type": "text",
          "data": "141\n- بنك الاستثمار ش.م.ع (شركة مساهمة عامة)"
        }
      ]
    },
    {
