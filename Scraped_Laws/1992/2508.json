  "preamble": "",
  "articles": [
    {
      "number": "2  ينشر هذا المرسوم ويبلغ حيث تدعو الحاجة.",
      "content": [
        {
          "type": "text",
          "data": "عدلت الرسوم على المصنوعات السكرية ذات المنشأ السوري و او العربي ويستوفي عن كل كيلوغرام من هذه المصنوعات الرسم المبين الى جانب كل منها:"
        },
        {
          "type": "text",
          "data": "اسم المصنوعات                           الرسم ل.ل."
        },
        {
          "type": "text",
          "data": "- الملبس وجميع انواع الملبسات                  30"
        },
        {
          "type": "text",
          "data": "- المربيات على اختلاف انواعها (جافة ومائعة) 25"
        },
        {
          "type": "text",
          "data": "- شراب السكر والشربات على اختلاف انواعها 25"
        },
        {
          "type": "text",
          "data": "- نوكا - قضامة على سكر                        21"
        },
        {
          "type": "text",
          "data": "- شوكولا على اختلافها                           21"
        },
        {
          "type": "text",
          "data": "- حلاوة طحينة                                    20"
        },
        {
          "type": "text",
          "data": "- خشافات                                          15"
        },
        {
          "type": "text",
          "data": "- بسكويت سادة او محشي                         30"
        },
        {
          "type": "text",
          "data": "- راحة الحلقوم                                     25"
        },
        {
          "type": "text",
          "data": "- حلاوة سكرية                                     21"
        },
        {
          "type": "text",
          "data": "- كارامل ودربس                                   21"
        },
        {
          "type": "text",
          "data": "- مربى وشراب                                    25"
        }
      ]
    },
    {
      "number": "1",
      "content": [
        {
          "type": "text",
          "data": "ينشر هذا المرسوم ويبلغ حيث تدعو الحاجة."
        }
      ]
    }
  ],
