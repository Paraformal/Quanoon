        },
        {
          "type": "text",
          "data": "11ــ"
        },
        {
          "type": "text",
          "data": "12ــ"
        },
        {
          "type": "text",
          "data": "13ــ شركة ليـبانيز ديلرز ش.م.ل."
        },
        {
          "type": "text",
          "data": "14ــ"
        },
        {
          "type": "text",
          "data": "15ــ رويال فاينانـشيلز ش.م.ل."
        },
        {
          "type": "text",
          "data": "16ــ ماستر كابـيتال غروب ش.م.ل."
        },
        {
          "type": "text",
          "data": "17ــ شركة الدقة للإستـثمار والوساطة الـمالية ش.م.ل."
