  "articles": [
    {
      "number": "3 \r\nمادة 3:\r\nينشر هذا المرسوم في الجريدة الرسمية ويبلغ حيث  تدعو الحاجة",
      "content": [
        {
          "type": "text",
          "data": "يجوز لمحتسبي الخزينة المبينين فيما يلي ان يؤمنوا بنفسهم"
        },
        {
          "type": "text",
          "data": "قبض الاموال التي يشرفون على ادارتها ودفعها."
        },
        {
          "type": "text",
          "data": "- محتسب قضاء بشري"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء راشيا"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء الهرمل"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء البقاع الغربي"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء حاصبيا"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء بنت جبيل"
        },
        {
          "type": "text",
          "data": "- محتسب قضاء مرجعيون"
        }
      ]
    },
    {
      "number": "1  (عدلت بموجب مرسوم 9880 / 2013)",
      "content": [
        {
          "type": "text",
          "data": "مادة 2:\r\nتلغى النصوص  السابقة المخالفة احكام هذا المرسوم او لا\r\nتتفق مع مضمونه"
        }
      ]
    },
    {
