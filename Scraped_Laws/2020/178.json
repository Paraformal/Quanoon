  },
  "preamble": "إن وزيرة الشباب والرياضة،\nبناء على الـمرسوم رقم 6157 تاريخ 21/1/2020(تشكيل الحكومة\n(\n,\nبناء على البند (1) من الـمادة 69 من الدستور لا سيما أحكام الفقرة (أ\n(\n,\nبناء على القانون رقم 247/2000 تاريخ 7/8/2000(دمج وإلغاء وإنشاء وزارات ومجالس),\nبناء على القانون رقم 629 تاريخ 20/11/2004(تنظيم وزارة الشباب والرياضة\n(\n,\nبناء على الـمرسوم رقم 4481 تاريخ 27/10/2016 (تنظيم الحركة الرياضية والشبابية والكشفية\n(\n,\nبناء على كتاب الاتحاد اللبناني للسباحة الـمسجل بالرقم 1815/د تاريخ 25/9/2020\n,\nبناء على اقتراح مدير عام الشباب والرياضة,\nيقرر ما يلي\n:",
  "articles": [
    {
      "number": "2 ينشر هذا القرار ويبلغ حيث تدعو الحاجة.",
      "content": [
        {
          "type": "text",
          "data": "تحدّد الجمعيات الرياضية الـمستمرة بتاريخ صدور هذا القرار في استيفاء الشروط القانونية والفنية الخاصة بلعبة «السباحة»، الـمرخصة بها ضمن ألعابها والـمنضمة الى «الاتحاد اللبناني للسباحة»، كما يلي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "أ. محافظة جبل لبنان"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "1"
        },
        {
          "type": "text",
          "data": "ـ نادي مون لاسال الرياضي ــ عين سعاده."
        },
        {
          "type": "text",
          "data": "2"
        },
        {
          "type": "text",
          "data": "ـ نادي كولينا كونتري كلوب ــ الـمطيلب."
        },
        {
          "type": "text",
          "data": "3 ــ نادي الدلب كونتري كلوب ــ بكفيا الـمحيدثة."
        },
        {
          "type": "text",
          "data": "4"
        },
        {
          "type": "text",
          "data": "ـ جمعية سبرينغ هيلز كونتري كلوب ــ الدكوانة مار روكز."
        },
        {
          "type": "text",
          "data": "5"
        },
        {
          "type": "text",
          "data": "ـ نادي مطيلب كونتري كلوب ــ الـمطيلب."
        },
        {
          "type": "text",
          "data": "6"
        },
        {
          "type": "text",
          "data": "ــ نادي غولدن بيتش ــ أشمون ــ أنطلياس."
        },
        {
          "type": "text",
          "data": "7"
        },
        {
          "type": "text",
          "data": "ــ نادي اللويزة ــ زوق مصبح."
        },
        {
          "type": "text",
          "data": "8"
        },
        {
          "type": "text",
          "data": "ــ نادي الجمهور الرياضي ــ اللويزة."
        },
        {
          "type": "text",
          "data": "9"
        },
        {
          "type": "text",
          "data": "ــ نادي أكوا مارينا الرياضي ــ كفرياسين"
        },
        {
          "type": "text",
          "data": "10"
        },
        {
          "type": "text",
          "data": "ــ نادي هوليداي بيتش الرياضي ــ ذوق مصبح"
        },
        {
          "type": "text",
          "data": "11"
        },
        {
          "type": "text",
          "data": "ــ نادي أبناء الخليج الرياضي ــ جونيه"
        },
        {
          "type": "text",
          "data": "12"
        },
        {
          "type": "text",
          "data": "ــ نادي صفرا مارين الرياضي ــ الصفرا"
        },
        {
          "type": "text",
          "data": "13"
        },
        {
          "type": "text",
          "data": "ــ نادي ساتيليتي الرياضي ــ فيطرون"
        },
        {
          "type": "text",
          "data": "14"
        },
        {
          "type": "text",
          "data": "ــ جمعية بيل أوريزون كونتري كلوب الرياضية ــ أدما"
        },
        {
          "type": "text",
          "data": "15"
        },
        {
          "type": "text",
          "data": "ــ نادي ديكاتلون الرياضي ــ أدونيس"
        },
        {
          "type": "text",
          "data": "16"
        },
        {
          "type": "text",
          "data": "ــ النادي اللبناني للسيارات والسياحة ــ الكسليك"
        },
        {
          "type": "text",
          "data": "ب. محافظة بيروت"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "17"
        },
        {
          "type": "text",
          "data": "ــ نادي النجاح الرياضي"
        },
        {
          "type": "text",
          "data": "18"
        },
        {
          "type": "text",
          "data": "ــ نادي الجزيرة الرياضي"
        },
        {
          "type": "text",
          "data": "19"
        },
        {
          "type": "text",
          "data": "ــ نادي أوركا"
        },
        {
          "type": "text",
          "data": "20"
        },
        {
          "type": "text",
          "data": "ــ نادي العهد الرياضي"
        },
        {
          "type": "text",
          "data": "21"
        },
        {
          "type": "text",
          "data": "ــ نادي 1875 الرياضي"
        },
        {
          "type": "text",
          "data": "ت. محافظة الشمال"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "22"
        },
        {
          "type": "text",
          "data": "ــ نادي الـميناء الرياضي ــ طرابلس"
        },
        {
          "type": "text",
          "data": "23"
        },
        {
          "type": "text",
          "data": "ــ نادي لاس سليناس الرياضي ــ أنفه"
        },
        {
          "type": "text",
          "data": "24"
        },
        {
          "type": "text",
          "data": "ــ نادي بالـما الرياضي ــ راس مسقا"
        },
        {
          "type": "text",
          "data": "ث. محافظة البقاع"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "25"
        },
        {
          "type": "text",
          "data": "ــ نادي الرحاب للسباحة ــ معلقة/زحلة"
        },
        {
          "type": "text",
          "data": "ج. محافظة الجنوب"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "26"
        },
        {
          "type": "text",
          "data": "ــ نادي فوربي الرياضي ــ صيدا"
        },
        {
          "type": "text",
          "data": "27"
        },
        {
          "type": "text",
          "data": "ــ نادي الفجر الرياضي ــ صيدا."
        }
      ]
    },
    {
      "number": "1",
      "content": [
        {
          "type": "text",
          "data": "ينشر هذا القرار ويبلغ حيث تدعو الحاجة"
        },
        {
          "type": "text",
          "data": "."
        }
      ]
    }
  ],
  "signature": "بيروت في 2 كانون الأول 2020\nوزيرة الشباب والرياضة\nفارتينيه أوهانيان"
