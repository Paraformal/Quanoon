  "preamble": "إن رئيس الجمهورية،\nبناء على الدستور،\nبناء على القانون رقم 389 تاريخ 12/1/1995 (انشاء الـمجلس الاقتصادي\nوالاجتماعي) الـمعدل بالقانون رقم 533 تاريخ 24/7/1996،\nبناء على الـمرسوم النافـذ حـكـمـاً رقــم 2 تــاريــخ 8/2/2007\nتحديد الهيئات الاكثر تمثيلاً للقطاعات التي يتألف منها الـمجلس الاقتصادي والاجتماعي\n(\n،\nبناء على اقتراح رئيس مجلس الوزراء،\nوبعد موافقة مجلس الوزراء بتاريخ 12/10/2017،\nيرسم ما يأتي:",
  "articles": [
    {
      "number": "2 ينشر هذا الـمرسوم ويبلغ حيث تدعو الحاجة ويعمل به فور نشره في الجريدة الرسمية.",
      "content": [
        {
          "type": "text",
          "data": "يؤلف الـمجلس الاقتصادي"
        },
        {
          "type": "text",
          "data": "والاجتماعي على الوجه التالي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "أولاً: عن اصحاب العمل"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "القطاع الصناعي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "جورج فوزي نصراوي"
        },
        {
          "type": "text",
          "data": "شوقي جرجي الدكاش"
        },
        {
          "type": "text",
          "data": "القطاع التجاري"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "محمد علي نزار شقير"
        },
        {
          "type": "text",
          "data": "عدنان محمد رمال"
        },
        {
          "type": "text",
          "data": "القطاع الزراعي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "يوسف محي الدين محي الدين"
        },
        {
          "type": "text",
          "data": "عمران وضاح فخري"
        },
        {
          "type": "text",
          "data": "القطاع الـمصرفي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "منصور قيصر بطيش"
        },
        {
          "type": "text",
          "data": "مازن محمود سويد"
        },
        {
          "type": "text",
          "data": "القطاع السياحي"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "وديع يوسف كنعان"
        },
        {
          "type": "text",
          "data": "جان طانس عبود"
        },
        {
          "type": "text",
          "data": "قطاع النقل"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "عبد الامير علي حسن نجده"
        },
        {
          "type": "text",
          "data": "قطاع الـمقاولين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "مارون جوزيف الحلو"
        },
        {
          "type": "text",
          "data": "قطاع التأمين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "مكس روجه زكار"
        },
        {
          "type": "text",
          "data": "القطاع الاستشفائي الخاص"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "طوني جرجس شمعون"
        },
        {
          "type": "text",
          "data": "القطاع التربوي الخاص"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "كيركوود الـمعروف بالخوري جان بول بطرس ابو غزالة"
        },
        {
          "type": "text",
          "data": "ثانياً: عن الـمهن الحرة"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "أ ــ عن الـمحامين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "جميل وفيق الحسامي"
        },
        {
          "type": "text",
          "data": "نجم محسن خطار"
        },
        {
          "type": "text",
          "data": "ب ــ عن الـمهندسين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "ايلي ابراهيم بصيبص"
        },
        {
          "type": "text",
          "data": "عبد الـمنعم محمود روحي علـم الدين"
        },
        {
          "type": "text",
          "data": "ج ــ عن الاطباء"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "عبد الامير علي فضل الله"
        },
        {
          "type": "text",
          "data": "مخائيل حنا جمل"
        },
        {
          "type": "text",
          "data": "د ــ عن أصحاب الصحف"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "عوني خير الدين الكعكي"
        },
        {
          "type": "text",
          "data": "هـ ــ عن الـمحررين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "الياس بشاره عون"
        },
        {
          "type": "text",
          "data": "و ــ عن الصيادلة"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "باسم محي الدين سرحال"
        },
        {
          "type": "text",
          "data": "ز ــ عن اطباء الاسنان"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "كارلوس ميلاد خير الله"
        },
        {
          "type": "text",
          "data": "ح ــ عن خبراء الـمحاسبة"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "ايلي دعيبس السغبيني"
        },
        {
          "type": "text",
          "data": "ثالثاً: عن النقابات"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "أ ــ عن العمال"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "بشاره كريم الاسمر"
        },
        {
          "type": "text",
          "data": "سعد الدين شفيق حميدي صقر"
        },
        {
          "type": "text",
          "data": "صادق حسن علويه"
        },
        {
          "type": "text",
          "data": "كريستيان نبيل باسيل"
        },
        {
          "type": "text",
          "data": "اسامه محمد الخنسا"
        },
        {
          "type": "text",
          "data": "بسام راجح طليس"
        },
        {
          "type": "text",
          "data": "محمد احمد قصابيه"
        },
        {
          "type": "text",
          "data": "فادي سليم اندريا"
        },
        {
          "type": "text",
          "data": "عصمت سعيد عبد الصمد"
        },
        {
          "type": "text",
          "data": "ماجد ابراهيم سعيفان"
        },
        {
          "type": "text",
          "data": "محمد بشار انور القدسي"
        },
        {
          "type": "text",
          "data": "حسن قاسم فقيه"
        },
        {
          "type": "text",
          "data": "ب ــ عن الاساتذة الجامعيين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "جورج ميشال الشويري"
        },
        {
          "type": "text",
          "data": "انيس نبيه بو ذياب"
        },
        {
          "type": "text",
          "data": "ج ــ عن الحرفيين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "عبد الكريم حسن حسن"
        },
        {
          "type": "text",
          "data": "جورج نقولا شلهوب"
        },
        {
          "type": "text",
          "data": "د ــ عن الـمعلـمين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "ــ التعليم الخاص: يوسف نجيب بسام"
        },
        {
          "type": "text",
          "data": "ــ التعليم العام: نزيه حسين الجباوي"
        },
        {
          "type": "text",
          "data": "هـ ــ عن الكتاب"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "محمد وجيه صبحي فانوس"
        },
        {
          "type": "text",
          "data": "و ــ عن الناشرين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "سميرة حسين عاصي"
        },
        {
          "type": "text",
          "data": "ز ــ عن مالكي الابنية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "جورج توفيق رباحيه"
        },
        {
          "type": "text",
          "data": "ح ــ عن الـمستأجرين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "ماري نقولا ناصيف"
        },
        {
          "type": "text",
          "data": "رابعاً: عن الجمعيات التعاونية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "جرجس حنا فخري"
        },
        {
          "type": "text",
          "data": "جورج الياس هراوي"
        },
        {
          "type": "text",
          "data": "خامساً: عن الـمؤسسات الاجتماعية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "أ ــ الـمؤسسات الاجتماعية غير الحكومية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "خالد توفيق الـمهتار"
        },
        {
          "type": "text",
          "data": "ليليان غازي جابر"
        },
        {
          "type": "text",
          "data": "زهيه رزق فرنجية"
        },
        {
          "type": "text",
          "data": "ب ــ الاتحادات النسائية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "فاطمة اقبال الشيخ محمد لبيب مراد"
        },
        {
          "type": "text",
          "data": "وفاء حليم عابد"
        },
        {
          "type": "text",
          "data": "."
        },
        {
          "type": "text",
          "data": "سادساً: عن أصحاب الفكر والكفاءة والاختصاص في الحقول الاقتصادية والاجتماعية والعلـمية والقانونية والثقافية والبيئية والفنية"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "شارل جوزف عربيد"
        },
        {
          "type": "text",
          "data": "فريد جورج فيليب البستاني"
        },
        {
          "type": "text",
          "data": "صلاح الدين نظام عسيران"
        },
        {
          "type": "text",
          "data": "نورا قابريل بايراقداريان"
        },
        {
          "type": "text",
          "data": "ماري لويز جورج بابويان"
        },
        {
          "type": "text",
          "data": "هاسميك كاربيس دانسكيان"
        },
        {
          "type": "text",
          "data": "غنى احمد مواس"
        },
        {
          "type": "text",
          "data": "فادي فخري علامه"
        },
        {
          "type": "text",
          "data": "غريتا حبيب صعب"
        },
        {
          "type": "text",
          "data": "اندره امين العور"
        },
        {
          "type": "text",
          "data": "سابعاً: عن اللبنانيين الـمغتربين"
        },
        {
          "type": "text",
          "data": ":"
        },
        {
          "type": "text",
          "data": "احمد علي ناصر"
        },
        {
          "type": "text",
          "data": "سعيد اسعد فخري"
        },
        {
          "type": "text",
          "data": "محمد ابراهيم الجوزو"
        },
        {
          "type": "text",
          "data": "هلال ريمون حريز"
        },
        {
          "type": "text",
          "data": "جورج الياس عطالله"
        },
        {
          "type": "text",
          "data": "فيليب فؤاد بردويل"
        }
      ]
    },
