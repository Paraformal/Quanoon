        {
          "type": "text",
          "data": "2"
        },
        {
          "type": "text",
          "data": "3"
        },
        {
          "type": "text",
          "data": "4"
        },
        {
          "type": "text",
          "data": "5 - الاعتماد التجاري والعقاري ش.م.ل."
        },
        {
          "type": "text",
          "data": "6 - سوجيليز لبنان ش.م.ل."
        },
        {
          "type": "text",
